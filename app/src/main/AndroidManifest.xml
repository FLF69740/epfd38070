<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.epfd.csandroid">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_pos2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_pos2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.PresentationActivity"
            android:label="@string/connexion_presentation_btn"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity android:name=".formulary.PrivacyPolicyActivity" />
        <activity android:name=".firstpage.FirstPageActivity" />
        <activity android:name=".formulary.PasswordActivity" />
        <activity android:name=".formulary.FormularyActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- NOTIFICATION SERVICE -->
        <service android:name=".notifications.NotificationsService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>