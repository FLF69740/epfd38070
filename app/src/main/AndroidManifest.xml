<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.epfd.csandroid">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_pos2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_pos2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".event.EventFileActivity"
            android:label="@string/event_event_single"
            android:parentActivityName=".event.EventMainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".event.EventMainActivity" />
        </activity>
        <activity android:name=".eventcreator.EventCreatorStageListingActivity" />
        <activity
            android:name=".administrator.stageedition.FileStageCreatorActivity"
            android:label="@string/stage_creator_stage"
            android:parentActivityName=".administrator.stageedition.StageCreatorActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".administrator.stageedition.StageCreatorActivity" />
        </activity>
        <activity
            android:name=".administrator.stageedition.StageCreatorActivity"
            android:label="@string/stage_creator_stage_menu"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity
            android:name=".eventcreator.EventCreatorMenuActivity"
            android:label="@string/event_creator_event_menu"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity
            android:name=".eventcreator.EventPanelActivity"
            android:label="@string/event_creator_event"
            android:parentActivityName=".eventcreator.EventCreatorMenuActivity"
            android:windowSoftInputMode="adjustPan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".eventcreator.EventCreatorMenuActivity" />
        </activity>
        <activity
            android:name=".event.EventMainActivity"
            android:label="@string/event_event"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity
            android:name=".administrator.newsedition.FileNewsPhotoBackEndActivity"
            android:label="@string/news_creator_photo_backend"
            android:parentActivityName=".administrator.newsedition.NewsCreatorActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".administrator.newsedition.NewsCreatorActivity" />
        </activity>
        <activity
            android:name=".administrator.newsedition.FileNewsCreatorActivity"
            android:label="@string/news_creator_news_file"
            android:parentActivityName=".administrator.newsedition.NewsCreatorActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".administrator.newsedition.NewsCreatorActivity" />
        </activity>
        <activity
            android:name=".administrator.newsedition.NewsCreatorActivity"
            android:label="@string/news_creator_label"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity
            android:name=".notifications.NotificationWindowActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity
            android:name=".administrator.cakefridayedition.CakeFridayActivity"
            android:label="@string/cake_edition_label"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity
            android:name=".administrator.classroomsedition.ClassroomsEditionActivity"
            android:label="@string/classrooms_edition_label"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity
            android:name=".formulary.ContactActivity"
            android:label="@string/connexion_contact_btn"
            android:parentActivityName=".MainActivity"
            android:windowSoftInputMode="adjustPan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".presentation.PresentationActivity"
            android:label="@string/connexion_presentation_btn"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".formulary.PrivacyPolicyActivity"
            android:label="@string/navigation_drawer_policy"
            android:parentActivityName=".firstpage.FirstPageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".firstpage.FirstPageActivity" />
        </activity>
        <activity android:name=".firstpage.FirstPageActivity" />
        <activity android:name=".formulary.PasswordActivity" />
        <activity android:name=".formulary.FormularyActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- NOTIFICATION SERVICE -->
        <receiver
            android:name=".utils.MyAlarmReceiver"
            android:exported="true" />

        <service android:name=".notifications.NotificationsService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.epfd.csandroid"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>